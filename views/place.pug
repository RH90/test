html
	head
		title!=title
		style
			include styles.css
		script(type="text/javascript")
			include script.js
		script(type="text/javascript")
			include sorttable.js
	body(class="main")
		include head.pug
		div(class="top")
			div
				button(onclick="location.href='/place/add'",style="margin-right:10px")!="LÃ¤gg till Plats"
		div(class="tableFixHead",id="hideAll")
			table(class="sortable")
				thead
					tr
						th!="Name"
						th!="Type"
				each val in rows
					tr
						td
							a(href="/place/"+val.id)!=val.name
						td!=val.type